  <h1>Welcome To History Maps</h1>

<% if !session[:user_id]%>
  <%= link_to "Register/Sign In", '/auth/google_oauth2', method: :get %>
<%else%>
  <%= link_to "Sign Out", '/logout', method: :get %>
<%end%>


<%= form_with url: buildings_path, method: :get, local: true do |f| %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.submit "See History Near Me" %>
<% end %>

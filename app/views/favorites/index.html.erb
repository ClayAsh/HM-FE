<%@favorites.each do |f|%>
  <% if f.user_id == @session_user_id.to_i %>
    <%=f.title%></br>
    <% if !f.photo.nil? && f.photo.include?("https") %>
      <%= image_tag(f.photo) %></br>
    <%end%>
    <%if !f.other_titles.nil?%>
      <%=f.other_titles%></br>
    <%end%>
    <%if !f.details.nil?%>
      <%=f.details%></br>
    <%end%>
    <%if !f.pdf.nil?%>
      <%=link_to "A PDF is Available!", f.pdf %></br>
    <%end%>
  <%end%>
<%end%>